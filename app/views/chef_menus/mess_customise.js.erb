<% if (current_user.blank?) %>
  $("#login_signup_modals").html("<%= escape_javascript(render(partial: 'layouts/sign_in')) %>");
  $("#login_signup_modals").append("<%= escape_javascript(render(partial: 'layouts/sign_up')) %>");

  $('#sign-in-modal').modal('show');
  login_signup_modals()
  var geocoder = new google.maps.Geocoder();

  map_initialize(24.8061, 46.6908);
<%else%>
	<% if !current_user.mess_request.present? %>
		$('#mess-customise').html('<%= escape_javascript(render(partial: "customise_mess_modal")) %>');
		$('#customise-mess-modal').modal('show');
	<% else %>
		$('#custom-mess-availability').html('Already Joined');
	<% end %>
<%end%>